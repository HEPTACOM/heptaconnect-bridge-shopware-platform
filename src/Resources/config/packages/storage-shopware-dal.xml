        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\WebHttpHandlerPathAccessor">
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\WebHttpHandlerPathIdResolver" type="service"/>
        </service>
        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\WebHttpHandlerPathIdResolver"/>

        <service

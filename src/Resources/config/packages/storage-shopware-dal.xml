<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Action\Job\Get\JobGetActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\Job\JobGet"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\Support\Query\QueryIterator" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Action\Job\Create\JobCreateActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\Job\JobCreate"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\JobTypeAccessor" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Action\Route\Listing\ReceptionRouteListActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\Route\ReceptionRouteList"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\Support\Query\QueryIterator" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Action\RouteCapability\Overview\RouteCapabilityOverviewActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\RouteCapability\RouteCapabilityOverview"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Action\Route\Create\RouteCreateActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\Route\RouteCreate"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\RouteCapabilityAccessor" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Action\Route\Find\RouteFindActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\Route\RouteFind"

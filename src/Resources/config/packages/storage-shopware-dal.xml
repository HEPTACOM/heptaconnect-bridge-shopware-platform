<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\ReceptionRouteListActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\ReceptionRouteList"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\Support\Query\QueryIterator" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\RouteCreateActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\RouteCreate"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\RouteFindByTargetsAndTypeActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\RouteFindByTargetsAndType"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\RouteOverviewActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\RouteOverview"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
        </service>
        <service
            id="Heptacom\HeptaConnect\Storage\Base\Contract\RouteGetActionInterface"
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Action\RouteGet"
        >
            <argument id="Doctrine\DBAL\Connection" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\Support\Query\QueryIterator" type="service"/>
        </service>
        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\DalAccess"/>
        <service
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityMapper"
            id="Heptacom\HeptaConnect\Storage\Base\Contract\EntityMapperContract"
        >
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="heptaconnect_mapping_node.repository.patched" type="service"/>
            <argument id="heptaconnect_mapping.repository.patched" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
        </service>
        <service
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\MappingPersister\MappingPersister"
            id="Heptacom\HeptaConnect\Storage\Base\MappingPersister\Contract\MappingPersisterContract"
        >
            <argument type="service" id="heptaconnect_mapping.repository.patched"/>
            <argument type="service" id="Doctrine\DBAL\Connection"/>
        </service>
        <service
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Repository\JobRepository"
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Repository\JobRepositoryContract"
        >
            <argument id="heptaconnect_job.repository.patched" type="service"/>
            <argument id="heptaconnect_job_type.repository.patched" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
        </service>
        <service
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Repository\JobPayloadRepository"
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Repository\JobPayloadRepositoryContract"
        >
            <argument id="heptaconnect_job_payload.repository.patched" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\DalAccess" type="service"/>
        </service>
        <service
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Repository\MappingNodeRepository"
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Repository\MappingNodeRepositoryContract"
        >
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="heptaconnect_mapping_node.repository.patched" type="service"/>
            <argument id="heptaconnect_mapping.repository.patched" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
        </service>
        <service
            class="Heptacom\HeptaConnect\Storage\ShopwareDal\Repository\RouteRepository"
            id="Heptacom\HeptaConnect\Storage\Base\Contract\Repository\RouteRepositoryContract"
        >
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="heptaconnect_route.repository.patched" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor" type="service"/>
        </service>
        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor">
            <argument id="heptaconnect_entity_type.repository.patched" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
        </service>
        <service class="Heptacom\HeptaConnect\Storage\ShopwareDal\Support\Query\QueryIterator"/>
    </services>
</container>

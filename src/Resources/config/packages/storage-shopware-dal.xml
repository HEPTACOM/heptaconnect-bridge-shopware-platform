        </service>
        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\EntityTypeAccessor">
            <argument id="heptaconnect_entity_type.repository" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\StorageKeyGenerator" type="service"/>
            <argument id="Heptacom\HeptaConnect\Storage\ShopwareDal\ContextFactory" type="service"/>
        </service>
        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\JobTypeAccessor">
            <argument id="Doctrine\DBAL\Connection" type="service"/>
        </service>
        <service id="Heptacom\HeptaConnect\Storage\ShopwareDal\RouteCapabilityAccessor">
